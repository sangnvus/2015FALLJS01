<!-- Donut Chart Example -->
<div class="row">
    <!-- Categories -->
    <div class="col-lg-4 col-md-12">
        <div class="portlet portlet-green">
            <div class="portlet-heading">
                <div class="portlet-title">
                    <h4>Lĩnh vực</h4>
                </div>
                <div class="portlet-widgets">
                    <a onclick="return false" href="javascript:;"><i class="fa fa-refresh"></i></a>
                    <span class="divider"></span>
                    <a onclick="return false" data-toggle="collapse" data-parent="#accordion" href="#cate"><i class="fa fa-chevron-down"></i></a>
                </div>
                <div class="clearfix"></div>
            </div>
            <div id="cate" class="panel-collapse collapse in">
                <div class="portlet-body">
                    <div class="portlet-body">
                        <canvas id="doughnut" class="chart chart-doughnut"
                                chart-data="Categories.Data" chart-labels="Categories.Labels" chart-legend="true"></canvas>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Rank -->
    <div class="col-lg-4 col-md-12">
        <div class="portlet portlet-green">
            <div class="portlet-heading">
                <div class="portlet-title">
                    <h4>Hạng dự án</h4>
                </div>
                <div class="portlet-widgets">
                    <a onclick="return false" href="javascript:;"><i class="fa fa-refresh"></i></a>
                    <span class="divider"></span>
                    <a onclick="return false" data-toggle="collapse" data-parent="#accordion" href="#rank"><i class="fa fa-chevron-down"></i></a>
                </div>
                <div class="clearfix"></div>
            </div>
            <div id="rank" class="panel-collapse collapse in">
                <div class="portlet-body">
                    <div class="portlet-body">
                        <canvas id="donut_Rank" class="chart chart-doughnut"
                                chart-data="donut_Rank.Data" chart-labels="donut_Rank.Labels" chart-legend="true"></canvas>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Rate -->
    <div class="col-lg-4 col-md-12">
        <div class="portlet portlet-green">
            <div class="portlet-heading">
                <div class="portlet-title">
                    <h4>Tỉ lệ thành công</h4>
                </div>
                <div class="portlet-widgets">
                    <a onclick="return false" href="javascript:;"><i class="fa fa-refresh"></i></a>
                    <span class="divider"></span>
                    <a onclick="return false" data-toggle="collapse" data-parent="#accordion" href="#rate"><i class="fa fa-chevron-down"></i></a>
                </div>
                <div class="clearfix"></div>
            </div>
            <div id="rate" class="panel-collapse collapse in">
                <div class="portlet-body">
                    <div class="portlet-body">
                        <canvas id="donut_Rate" class="chart chart-doughnut"
                                chart-data="donut_Rate.Data" chart-labels="donut_Rate.Labels" chart-legend="true"></canvas>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end donut chart -->
<!-- begin top table -->
<div class="row">
    <!-- Top Backer -->
    <div class="col-lg-6">
        <div class="portlet portlet-green">
            <div class="portlet-heading">
                <div class="portlet-title">
                    <h4>Những người ủng hộ nhiều nhất</h4>
                </div>
                <div class="portlet-widgets">
                    <a onclick="return false" data-toggle="collapse" data-parent="#accordion" href="#transactionsPortlet"><i class="fa fa-chevron-down"></i></a>
                </div>
                <div class="clearfix"></div>
            </div>
            <div id="transactionsPortlet" class="panel-collapse collapse in">
                <div class="portlet-body">
                    <div class="table-responsive dashboard-demo-table">
                        <table class="table table-bordered table-striped table-hover">
                            <thead>
                                <tr>
                                    <th>Tên</th>
                                    <th>Số dự án</th>
                                    <th>Số tiền(VNĐ)</th>
                                    <th>Trạng thái</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="backer in TopBacker">
                                    <td class="col-text">{{backer.FullName}}</td>
                                    <td class="col-number">{{backer.TotalProject  | currency:"":0}}</td>
                                    <td class="col-number">{{backer.TotalPledgedAmount  | currency:"":0}}</td>
                                    <td class="col-text">
                                        <a class="btn btn-xs btn-green" ng-if="backer.Status == true">Bình thường</a>
                                        <a class="btn btn-xs btn-red" ng-if="backer.Status == false">Khóa</a>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end top backers -->
    <!-- Top Projects -->
    <div class="col-lg-6">
        <div class="portlet portlet-green">
            <div class="portlet-heading">
                <div class="portlet-title">
                    <h4>Những dự án thành công nhất</h4>
                </div>
                <div class="portlet-widgets">
                    <a onclick="return false" data-toggle="collapse" data-parent="#accordion" href="#transactionsPortlet2"><i class="fa fa-chevron-down"></i></a>
                </div>
                <div class="clearfix"></div>
            </div>
            <div id="transactionsPortlet2" class="panel-collapse collapse in">
                <div class="portlet-body">
                    <div class="table-responsive dashboard-demo-table">
                        <table class="table table-bordered table-striped table-hover">
                            <thead>
                                <tr>
                                    <th>Dự án</th>
                                    <th>Lĩnh vực</th>
                                    <th>Chủ dự án</th>
                                    <th>Mục tiêu(VNĐ)</th>
                                    <th>Quỹ(VNĐ)</th>
                                    <th>Trạng thái</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="project in TopProject">
                                    <td>{{project.Title}}</td>
                                    <td>{{project.Category}}</td>
                                    <td>{{project.CreatorEmail}}</td>
                                    <td>{{project.FundingGoal | currency:"":0}}</td>
                                    <td>{{project.CurrentFunded | currency:"":0}}</td>
                                    <td><a class="btn btn-xs btn-green">Approved</a></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end top Projects -->
</div>
<!-- end top table -->
