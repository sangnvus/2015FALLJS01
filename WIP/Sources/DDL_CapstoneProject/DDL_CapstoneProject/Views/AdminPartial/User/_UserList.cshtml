<div class="row">

    <div class="col-lg-12">

        <div class="portlet portlet-default">
            <div class="portlet-heading">
                <div class="portlet-title">
                    <h4>Danh sách Người dùng</h4>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="portlet-body">
                <div id="EditCategoryModal" class="table-responsive">
                    <form name="EditCategoryForm">
                        <table id="example-table" class="table table-striped table-bordered table-hover table-green"
                               datatable="ng" dt-options="dtOptions" dt-column-defs="dtColumnDefs">
                            <thead>
                                <tr>
                                    <th>STT</th>
                                    <th>Tên đầy đủ</th>
                                    <th>Email</th>
                                    <th>Số điện thoại</th>
                                    <th>Loại tài khoản</th>
                                    <th>Trạng thái</th>
                                    <th>Hành động</th>
                                </tr>
                            </thead>
                            <tbody>

                                <tr ng-repeat="user in userList.ListUser" class="even gradeC" ng-class-odd="'odd'" ng-class-even="'even'">
                                    <td>{{$index + 1}}</td>
                                    <td>{{user.FullName}}</td>
                                    <td>{{user.Email}}</td>
                                    <td>{{user.PhoneNumber}}</td>
                                    <td>{{user.LoginType}}</td>
                                    <td>
                                        <!-- <button class="btn btn-default">Inactive</button> -->
                                        <div ng-show="user.Status">
                                            <a href="/admin/#/userlist" class="btn btn-green">Đã kích hoạt</a>
                                        </div>
                                        <div ng-hide="user.Status">
                                            <a class="btn btn-default">Chưa kích hoạt</a>
                                        </div>
                                    </td>
                                    <td>
                                        <a href="/admin/#/userprofile/{{user.UserName}}"><button class="btn btn-blue">Xem</button></a>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </form>
                </div>
                <!-- /.table-responsive -->
            </div>
            <!-- /.portlet-body -->
        </div>
        <!-- /.portlet -->

