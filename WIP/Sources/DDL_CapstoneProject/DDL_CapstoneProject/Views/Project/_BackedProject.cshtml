<div id="items-projects">
    <section id="portfolio" class="container">
        <h4 class="pt30">Dự án đã ủng hộ:</h4>
        
        <ul class="list-inline fnt18 fnt-b">
            <li class=""><a class="c3" href="../#/project/backedProject">Dự án đã ủng hộ</a></li>
            <li class=""><a class="c-10" href="../#/project/backedProjectHistory">Lịch sử ủng hộ</a></li>
        </ul>
        <div ng-if="checkEmpty==0">Bạn chưa ủng hộ dự án nào</div>
        <ul class="portfolio-items col-4">
            <li class=" portfolio-item apps" ng-repeat="project in ListBackedProject">

                <div class="item-inner" ng-if="checkEmpty>0">
                    <div class=" item-image hover01 column">
                        <a href="/#/project/detail/{{project.ProjectCode}}"><figure><img class="img-fit" ng-src="\images\projects\{{project.ImageUrl}}" alt=""></figure></a>
                    </div>
                    <div class="item-title">
                        <h5><a a href="/#/project/detail/{{project.ProjectCode}}">{{project.Title}} </a></h5>
                    </div>
                    <div class="item-content">
                        <p class="fnt15">{{project.SubDescription}}</p>
                    </div>
                    <div class="item-footer">
                        <div class="mt10 mb5">
                            <a class="grey-dark" href=""><span class="glyphicon glyphicon-map-marker" aria-hidden="true">&nbsp;</span>{{project.Location}}</a>
                            &nbsp;&nbsp;
                            <a class="grey-dark" href=""><span class="glyphicon glyphicon-tags" aria-hidden="true">&nbsp;</span>{{project.Category}}</a>
                            &nbsp;&nbsp;
                        </div>
                        <div class="progress mt5 mb5">
                            <div class="progress mt5 mb5">
                                <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="0" aria-valuemin="{{project.CurrentFunded}}" aria-valuemax="100" ng-style="width: {{project.CurrentFunded}}%">
                                    <span class="sr-only">{{project.CurrentFunded}}% Complete (success)</span>
                                </div>
                            </div>
                        </div>
                        <ul class="list-inline">
                            <li><p class="fnt-b mt0 mb0 c00">{{project.CurrentFunded}}%</p><p class="mt0 mb0">ủng hộ</p></li>
                            <li><p class="fnt-b mt0 mb0 c00">{{project.CurrentFundedNumber |  number : fractionSize}}&#8363;</p><p class="mt0 mb0">gây quỹ</p></li>
                            <li><p class="fnt-b mt0 mb0 c00">{{project.ExpireDate}}</p><p class="mt0 mb0">ngày còn lại</p></li>
                        </ul>
                    </div>
                   
                </div>
            </li><!--/.portfolio-item-->

        </ul>
    </section><!--/#portfolio-->
</div>

<script>
    $(window).trigger('resize');
</script>