<div id="setting_container" class="paddingNavbar mt50">
    <div class="row pull-right">
        <div class="col-sm-12 col-sm-offset-0">
            <ul id="" class="list-inline fnt18 fnt-b">
                <li class=""><a class="c-3" href="../#/project/created">Dự án đã tạo</a></li>
                <li class=""><a class="c-10" href="../#/project/backed">Dự án đã ủng hộ</a></li>
                <li class=""><a class="c-10" href="../#/project/reminded">Dự án theo dõi</a></li>
            </ul>
        </div>
    </div>
</div>

<div id="items-projects">
    <section id="portfolio" class="container">
        <h4 class="pt30">Dự án đã tạo:</h4>
        <ul ng-if="checkEmpty == 0">Bạn chưa tạo dự án nào</ul>
        <h4 class="pt0" ng-show="checkEmptyDraft>0">Dự án nháp:</h4>
        <!--Draft/ Reject-->
        <ul class="portfolio-items col-4">
            <li class="portfolio-item apps" ng-repeat="project in ListCreatedProjectDraft">

                <div class="item-inner">
                    <div class=" item-image hover01 column">
                        <a href="/#/project/detail/{{project.ProjectCode}}"><figure><img class="img-fit" ng-src="\images\projects\{{project.ImageUrl}}" alt=""></figure></a>
                    </div>
                    <div class="item-title">
                        <h5><a href="/#/project/detail/{{project.ProjectCode}}">{{project.Title}}</a></h5>
                        <a class="btn btn-info btn-xs" href="/#/project/edit/{{project.ProjectCode}}">Sửa</a>
                        <a class="btn btn-danger btn-xs" href="" ng-click="deleteDraft($index)">Xóa</a>
                        <label class="label label-primary" ng-if="project.Status == 'pending'">Đang chờ kiểm duyệt</label>
                        <label class="label label-default" ng-if="project.Status == 'draft'">Dự án nháp</label>
                        <label class="label label-danger" ng-if="project.Status == 'rejected'">Đã bị từ chối</label>
                    </div>
                    <div class="item-content">
                        <p class="fnt15">{{project.SubDescription}}</p>
                    </div>
                    <div class="item-footer">
                        <div class="mt10 mb5 no-wrap text-hidden">
                            <a class="grey-dark" href="@Url.Content("/#/search?categoryid={{project.CategoryID}}")"><span class="glyphicon glyphicon-tags" aria-hidden="true">&nbsp;</span>{{project.Category}}</a>
                            &nbsp;&nbsp;
                            <span class="grey-dark"><span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span><span tooltip='{{project.Location}}'>{{project.Location}}</span></span>
                        </div>
                        @*<div class="progress mt5 mb5">
                                <div class="progress-bar progress-bar-success" role="progressbar" aria-valuemin="0" aria-valuenow="{{project.CurrentFunded}}" aria-valuemax="100" style="width: {{project.CurrentFunded}}%">
                                    <span class=" sr-only">
                                        {{project.CurrentFunded}}% Complete (success)
                                    </span>
                                </div>
                            </div>
                            <ul class="list-inline fnt15">
                                <li>
                                    <span class="fnt-b mt0 mb0 c00">{{project.CurrentFunded}}%</span>
                                    <span class="mt0 mb0">đã ủng hộ</span>
                                </li>
                                <li>
                                    <span class="fnt-b mt0 mb0 c00">{{project.CurrentFundedNumber| number : fractionSize}}&#8363;</span>
                                    <span class="mt0 mb0">đã gây quỹ</span>
                                </li>
                            </ul>*@
                    </div>
                </div>
            </li><!--/.portfolio-item-->
        </ul>
        <div class="clearfix"></div>
        @*<div class="form-actions txt-c">
                <a class="btn btn-transparent btn-sm" href="#">Thêm</a>
            </div>*@
        <!--Running-->
        <h4 class="pt30" ng-if="checkEmptyApproved>0">Dự án đang hoạt động:</h4>
        <ul class="portfolio-items col-4">

            <li class="portfolio-item apps" ng-repeat="project in ListCreatedProjectApproved">
                <div class="item-inner">
                    <div class="item-image hover01 column">
                        <a href="/#/project/detail/{{project.ProjectCode}}"><figure><img class="img-fit" ng-src="\images\projects\{{project.ImageUrl}}" alt=""></figure></a>
                    </div>
                    <div class="item-title">
                        <h5><a href="/#/project/detail/{{project.ProjectCode}}">{{project.Title}}</a></h5>
                    </div>
                    <div class="item-content">
                        <a class="btn btn-primary btn-xs" href="/#/project/listBacker/{{project.ProjectCode}}">Danh sách ủng hộ</a>
                        <a class="btn btn-info btn-xs" href="/#/project/edit/{{project.ProjectCode}}">Sửa</a>
                        <p class="fnt15">{{project.SubDescription}}</p>
                    </div>
                    <div class="item-footer">
                        <div class="mt10 mb5 no-wrap text-hidden">
                            <a class="grey-dark" href=""><span class="glyphicon glyphicon-tags" aria-hidden="true">&nbsp;</span>{{project.Category}}</a>
                            &nbsp;&nbsp;
                            <span class="grey-dark"><span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span><span tooltip='{{project.Location}}'>{{project.Location}}</span></span>
                        </div>
                        <div class="progress mt5 mb5">
                            <div class="progress-bar progress-bar-success" role="progressbar" aria-valuemin="0" aria-valuenow="{{project.CurrentFunded}}" aria-valuemax="100" style="width: {{project.CurrentFunded}}%">
                                <span class="sr-only">{{project.CurrentFunded}}% Complete (success)</span>
                            </div>
                        </div>
                        <ul class="list-inline fnt15">
                            <li>
                                <span class="fnt-b mt0 mb0 c00">{{project.CurrentFundedNumber| number : fractionSize}}&#8363;({{project.CurrentFunded}}%)</span>
                                <span class="mt0 mb0">đã gây quỹ</span>
                            </li>
                            @*<li>
                                    <span class="fnt-b mt0 mb0 c00">{{project.CurrentFunded}}%</span>
                                    <span class="mt0 mb0">đã ủng hộ</span>
                                </li>*@
                        </ul>
                        <ul class="list-inline fnt15">
                            <li>
                                <span class="fnt-b mt0 mb0 c00">{{project.ExpireDate}}</span>
                                <span class="mt0 mb0">ngày còn lại</span>
                            </li>
                        </ul>
                    </div>
                </div>
            </li><!--/.portfolio-item-->

        </ul>

        <div class="clearfix"></div>
        @*<div class="form-actions txt-c">
                <a class="btn btn-transparent btn-sm" href="#">Thêm</a>
            </div>*@
        <!--Expired-->
        <h4 class="pt30" ng-if="checkEmptyExpired>0">Dự án đã kết thúc:</h4>
        <ul class="portfolio-items col-4">

            <li class="portfolio-item apps" ng-repeat="project in ListCreatedProjectExpired">

                <div class="item-inner">
                    <div class=" item-image hover01 column">
                        <figure><img class="img-fit" ng-src="\images\projects\{{project.ImageUrl}}" alt=""></figure>
                    </div>
                    <div class="item-title">
                        <h5><a href="/#/project/detail/{{project.ProjectCode}}">{{project.Title}}</a></h5>
                        <a class="btn btn-primary btn-xs" href="/#/project/listBacker/{{project.ProjectCode}}">Danh sách ủng hộ</a>
                    </div>
                    <div class="item-content">
                        <p class="fnt15">{{project.SubDescription}}</p>
                    </div>
                    <div class="item-footer">
                        <div class="mt10 mb5 no-wrap text-hidden">
                            <a class="grey-dark" href=""><span class="glyphicon glyphicon-tags" aria-hidden="true">&nbsp;</span>{{project.Category}}</a>
                            &nbsp;&nbsp;
                            <span class="grey-dark"><span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span><span tooltip='{{project.Location}}'>{{project.Location}}</span></span>
                        </div>
                        <div class="progress mt5 mb5">

                            <div class="progress-bar progress-bar-success" role="progressbar" aria-valuemin="0" aria-valuenow="{{project.CurrentFunded}}" aria-valuemax="100" style="width: {{project.CurrentFunded}}% " ng-if="project.CurrentFunded >= 100">
                                <span class="sr-only">{{project.CurrentFunded}}% Complete (Thành công)</span>
                            </div>

                            <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuemin="0" aria-valuenow="{{project.CurrentFunded}}" aria-valuemax="100" style="width: {{project.CurrentFunded}}% " ng-if="project.CurrentFunded < 100">
                                <span class="sr-only">{{project.CurrentFunded}}% Complete (thất bại)</span>
                            </div>

                        </div>
                        <ul class="list-inline fnt15">
                            <li>
                                <span class="fnt-b mt0 mb0 c00">{{project.CurrentFundedNumber| number : fractionSize}}&#8363;({{project.CurrentFunded}}%)</span>
                                <span class="mt0 mb0">đã gây quỹ</span>
                            </li>
                            @*<li>
                                    <span class="fnt-b mt0 mb0 c00">{{project.CurrentFunded}}%</span>
                                    <span class="mt0 mb0">đã ủng hộ</span>
                                </li>*@
                            @*<li>
                                    <span class="fnt-b mt0 mb0 c00">{{project.ExpireDate}}</span>
                                    <span class="mt0 mb0">ngày còn lại</span>
                                </li>*@
                        </ul>
                        <ul class="list-inline fnt15">
                            <li>
                                <span class="fnt-b mt0 mb0 label label-success" ng-if="project.CurrentFunded >= 100">Thành công</span>
                                <span class="fnt-b mt0 mb0 label label-danger" ng-if="project.CurrentFunded < 100">Thất bại</span>
                                @*<span class="mt0 mb0">{{project.CreatedDate | date : format : shortDate}}</span>*@
                            </li>
                        </ul>
                    </div>
                </div>
            </li><!--/.portfolio-item-->


        </ul>

        <div class="clearfix"></div>
        @*<div class="form-actions txt-c">
                <a class="btn btn-transparent btn-sm" href="#">Thêm</a>
            </div>*@


    </section><!--/#portfolio-->
</div>
<script>
    $(window).trigger('resize');
</script>
