<!-- begin ADVANCED TABLES ROW -->
<div class="row">

    <div class="col-lg-12">

        <div class="portlet portlet-default">
            <div class="portlet-heading">
                <div class="portlet-title">
                    <h4>New Projects List</h4>
                </div>
                <div class="portlet-widgets">
                    <a href="#" class="btn btn-blue btn-sm">Export</a>
                    <a data-toggle="collapse" data-parent="#accordion" href="#NewProjectTable"><i class="fa fa-chevron-down"></i></a>
                </div>
                <div class="clearfix"></div>
            </div>
            <div id="NewProjectTable">
                <div class="portlet-body">
                    <div class="table-responsive">
                        <table id="example-table" class="table table-striped table-bordered table-hover table-green"
                               datatable="ng" dt-options="dtOptions" dt-column-defs="dtColumnDefs">
                            <thead>
                                <tr>
                                    <th>Code</th>
                                    <th>Title</th>
                                    <th>Category</th>
                                    <th>Creator</th>
                                    <th>Fund Goal</th>
                                    <th>Expired Date</th>
                                    <th>Status</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="project in ProjectList" class="odd">
                                    <td>{{project.ProjectCode}}</td>
                                    <td>{{project.Title}}</td>
                                    <td>{{project.Category}}</td>
                                    <td class="center">{{project.CreatorEmail}}</td>
                                    <td class="center">{{project.FundingGoal}}</td>
                                    <td class="center">{{project.ExpireDate | date:"dd/MM/yyyy"}}</td>
                                    <td><span class="label label-primary center-block">{{project.Status}}</span></td>
                                    <td class="center">
                                        <div class="btn-group">
                                            <a href="@Url.Content("/admin/#/project/{{project.ProjectCode}}")" class="btn btn-primary btn-sm" type="button" id="dropdownMenu1"
                                               data-toggle="tooltip" data-placement="bottom" title="View">
                                                <i class="fa fa-eye"></i>
                                            </a>
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.portlet-body -->
            </div>
        </div>
        <!-- /.portlet -->

    </div>
    <!-- /.col-lg-12 -->

</div>

<!-- /.page-content -->
<!-- /#page-wrapper -->
<!-- end MAIN PAGE CONTENT -->